{
	"name": "@envindavsorg/ts-safe-path",
	"version": "1.0.4",
	"description": "Type-safe nested object access and manipulation for TypeScript \uD83D\uDE80",
	"packageManager": "pnpm@9.0.0",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"build": "tsc",
		"typecheck": "tsc --noEmit",
		"test": "jest",
		"test:watch": "jest --watch",
		"prepublishOnly": "npm run build && npm test",
		"lint": "biome check .",
		"lint:fix": "biome check --write ."
	},
	"keywords": [
		"typescript",
		"type-safe",
		"object",
		"nested",
		"path",
		"lodash",
		"get",
		"set",
		"utility"
	],
	"author": "Cuzeac Florin",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/envindavsorg/ts-safe-path.git"
	},
	"publishConfig": {
		"registry": "https://npm.pkg.github.com"
	},
	"bugs": {
		"url": "https://github.com/envindavsorg/ts-safe-path/issues"
	},
	"homepage": "https://github.com/envindavsorg/ts-safe-path#readme",
	"devDependencies": {
		"@types/jest": "^30.0.0",
		"@biomejs/biome": "2.1.3",
		"jest": "^30.0.5",
		"ts-jest": "^29.4.1",
		"typescript": "^5.9.2"
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"engines": {
		"node": ">=14.0.0"
	},
	"jest": {
		"preset": "ts-jest",
		"testEnvironment": "node",
		"testMatch": [
			"**/tests/**/*.test.ts"
		]
	}
}
